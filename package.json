  },
  "dependencies": {
    "auth0-js": "^8.8.0",
    "bourbon": "^4.3.4",
    "bourbon-neat": "^1.9.0",
    "dateformat": "^2.0.0",
    "file-icons-js": "github:websemantics/file-icons-js",
    "fine-uploader-wrappers": "^1.0.1",
    "graphql": "^0.10.5",
    "inline-attachment": "^2.0.3",
    "moment": "^2.19.1",
    "normalize": "^0.3.1",
    "pretty-file-icons": "^2.2.1",
    "react": "^15.6.1",
    "react-breadcrumbs": "^1.6.6",
    "react-dom": "^15.6.1",
    "react-markdown": "^2.5.0",
    "react-relay": "^1.1.0",
    "react-router-dom": "^4.1.1",
    "react-scroll": "^1.5.4",
    "react-tag-input": "^4.7.2",
    "react-transition-group": "^2.2.1",
    "showdown": "^1.7.3",
    "simplemde": "^1.11.2",
    "sweetalert-react": "^0.4.10",
    "uuid": "^3.1.0",
    "ws": "^3.1.0",
    "xmlhttprequest": "^1.8.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "jest",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql",
    "relay-test": "relay-compiler --src ./__tests__ --schema ./schema.graphql",
    "update-schema": "graphql get-schema",
    "update-json-schema": "babel-node ./scripts/updateSchema.js",
    "build-tests": "BABEL_ENV='development' babel-node ./scripts/buildTests.js",
    "mock": "node __mocks__/react-relay.js",
    "polyfills": "babel-node ./config/polyfills.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/js/**.*js",
      "!src/js/**.*graphql.js",
      "!src/js/mutations/**/*.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/__tests__/setupFile.js"
    ],
    "testMatch": [
      "<rootDir>/**/__tests__/**/*.test.js?(x)"
    ],
    "verbose": false,
    "browser": true,
    "globals": {
      "__DEV__": true
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^react-markdown$": "react-markdown",
      "^Components[/](.+)": "<rootDir>/src/js/components/$1",
      "^Mutations[/](.+)": "<rootDir>/src/js/mutations/$1",
      "^JS[/](.+)": "<rootDir>/src/js/$1"
    },
    "setupTestFrameworkScriptFile": "./__tests__/localStorage.js"
  },
  "babel": {
    "presets": [
      "react-app",
      "es2015"
    ],
    "plugins": [
      "relay",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
